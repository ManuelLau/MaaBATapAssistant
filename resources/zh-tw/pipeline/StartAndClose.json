{
	"StartGame": {
		"package": "com.nexon.bluearchive/.MxUnityPlayerActivity"
	},
	"CloseGame": {
		"package": "com.nexon.bluearchive"
	},
	"RestartGame": {
		"package": "com.nexon.bluearchive"
	},

	"StartUp@Start": {
		"interrupt": [
			"StartUp@WaitLoading",
			"StartUp@ClientUpdatePopUp",
			"StartUp@UpdatePopUp",
			"StartUp@NexonLoginPopUp",
			"RefreshGame@MaintenancePopUp",
			"StartUp@TitleScreen",
			"StartUp@DailyCheckIn",
			"StartUp@EventCheckIn",
			"DisconnectPopup",
			"ClosePopup",
			"CloseReward",
			"ClickHomeButton"
		]
	},
	"StartUp@WaitLoading": {
		"text": [
			"Load",
			"Initialize",
			"重置遊戲資料",
			"正在更新",
			"驗證下載",
			"正在解壓"
		]
	},
	"StartUp@TitleScreen": {
		"next": "StartUp@NexonCheckAnnouncementCloseButtonEntry",
		"doc": "国际服-覆写登录流程。从标题页面进入到主页，固定会弹公告，检测公告弹窗的关闭按钮"
	},
	"StartUp@NexonCheckAnnouncementCloseButtonEntry": {
		"next": "StartUp@NexonCheckAnnouncementCloseButton",
		"interrupt": [
			"StartUp@WaitLoading",
			"StartUp@NexonLoginPopUp",
			"StartUp@UpdatePopUp",
			"RefreshGame@MaintenancePopUp",
			"StartUp@DailyCheckIn",
			"StartUp@EventCheckIn",
			"StartUp@NexonTitleScreen"
		],
		"timeout": 120000
	},
	"StartUp@NexonCheckAnnouncementCloseButton": {
		"recognition": "TemplateMatch",
		"template": "CloseButtonNexon.png",
		"green_mask": true,
		"action": "Click"
	},
	"StartUp@NexonTitleScreen": {
		"recognition": "TemplateMatch",
		"template": "MenuButton.png",
		"roi": [16, 606, 106, 54],
		"green_mask": true,
		"pre_delay": 1000,
		"action": "Click",
		"target": [640, 360, 0, 0],
		"post_delay": 1000
	},
	"StartUp@NexonLoginPopUp": {
		"recognition": "OCR",
		"text": "今日不再顯示",
		"action": "Click",
		"doc": "国际服-关闭活动提示弹窗"
	},
	"StartUp@EventCheckIn": {
		"text": "剩餘時間"
	},
	"StartUp@UpdatePopUp": {
		"text": ["下載必要內容", "需要下載遊戲"]
	},
	"StartUp@ClientUpdatePopUp": {
		"text": "到有新版本"
	}
}